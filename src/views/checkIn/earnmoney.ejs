        <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
        <script>
            const mainTabs = document.querySelectorAll(".sys-tab-item");
            const mainContents = document.querySelectorAll(".sys-tab-content");

            mainTabs.forEach((tab) => {
                tab.addEventListener("click", () => {
                    mainTabs.forEach((t) => t.classList.remove("active"));
                    tab.classList.add("active");

                    mainContents.forEach((c) => c.classList.remove("active"));

                    const target = tab.getAttribute("data-tab");
                    document.getElementById(target).classList.add("active");

                    window.scrollTo(0, 0);
                });
            });

            const rebateTabs = document.querySelectorAll(".sys-tab-btn");
            const rebateSections = document.querySelectorAll(
                ".sys-rebate-section",
            );

            rebateTabs.forEach((tab) => {
                tab.addEventListener("click", () => {
                    const targetSection = document.getElementById(
                        tab.getAttribute("data-tab"),
                    );

                    rebateTabs.forEach((tab) => tab.classList.remove("active"));
                    tab.classList.add("active");

                    rebateSections.forEach((section) =>
                        section.classList.remove("active"),
                    );
                    targetSection.classList.add("active");
                });
            });

            document.querySelectorAll(".accordion-btn").forEach((button) => {
                button.addEventListener("click", () => {
                    const content = button.nextElementSibling;
                    const icon = button.querySelector(".accordion-icon");

                    // Close all other accordions
                    document
                        .querySelectorAll(".accordion-content")
                        .forEach((item) => {
                            if (item !== content) {
                                item.style.maxHeight = null;
                                item.previousElementSibling
                                    .querySelector(".accordion-icon")
                                    .classList.remove("rotate-180");
                            }
                        });

                    // Toggle current
                    if (content.style.maxHeight) {
                        content.style.maxHeight = null;
                        icon.classList.remove("rotate-180");
                    } else {
                        content.style.maxHeight = content.scrollHeight + "px";
                        icon.classList.add("rotate-180");
                    }
                });
            });
        </script>
        <script>
            function showTab(tab) {
                const contents = document.querySelectorAll(".tab-content");
                contents.forEach((content) => content.classList.add("hidden"));
                const tabButtons = document.querySelectorAll(".tab-button");
                tabButtons.forEach((button) =>
                    button.classList.remove("bg-purple-600", " text-black"),
                );
                document
                    .getElementById(`${tab}-content`)
                    .classList.remove("hidden");
                document
                    .getElementById(`${tab}-tab`)
                    .classList.add("bg-purple-600", " text-black");
            }
            document
                .getElementById("mine-tab")
                .classList.add("bg-purple-600", " text-black");

            let inviteCode = "";

            // Fetch user info on page load
            async function fetchInviteCode() {
                try {
                    const response = await axios.get("/api/webapi/GetUserInfo");
                    const userData = response?.data?.data || {};
                    inviteCode = userData.code || "";
                } catch (err) {
                    console.error("Failed to fetch invite code:", err);
                }
            }

            window.addEventListener("load", fetchInviteCode);

            function copyLink() {
                if (!inviteCode) {
                    Swal.fire("Error", "Invite code not ready yet!", "error");
                    return;
                }

                // Automatically get the current domain
                const domain = window.location.origin;
                const inviteLink = `${domain}/register?invitationCode=${inviteCode}`;

                navigator.clipboard
                    .writeText(inviteLink)
                    .then(() =>
                        Swal.fire(
                            "Copied!",
                            "Invite link copied to clipboard âœ…",
                            "success",
                        ),
                    )
                    .catch((err) => console.error(err));
            }

            function copyCode() {
                if (!inviteCode) {
                    Swal.fire("Error", "Invite code not ready yet!", "error");
                    return;
                }
                navigator.clipboard
                    .writeText(inviteCode)
                    .then(() =>
                        Swal.fire(
                            "Copied!",
                            "Invite code copied âœ…",
                            "success",
                        ),
                    )
                    .catch((err) => console.error(err));
            }
        </script>

