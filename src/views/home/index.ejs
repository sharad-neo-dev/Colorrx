    scrollInner.appendChild(card);
  });

  // Append scroll only if NOT Sport
  if (cat !== "Sport") {
    scrollWrapper.appendChild(scrollInner);
    section.appendChild(scrollWrapper);
  } else {
    // Optional: For Sport, show grid layout instead of scroll
    const grid = document.createElement("div");
    grid.className = "";
    grid.appendChild(scrollInner);
