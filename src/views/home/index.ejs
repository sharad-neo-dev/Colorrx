   <script>
    // --- CONFIGURATION ---
    const API_ENDPOINT = '/api/webapi/pop';
    const DONT_SHOW_AGAIN_KEY = 'popups_dont_show';

    // --- 1. Utility Functions ---
    function shouldShowPopups() {
        return localStorage.getItem(DONT_SHOW_AGAIN_KEY) !== 'true';
    }

    // --- 2. Main Popup Display Logic ---
    function showNextPopup(popups) {
        if (!popups || popups.length === 0) return;

        const currentPopup = popups.shift();

        const popupHtml = `
            <div id="popup-overlay-${currentPopup.pop_id}" 
                data-pop-id="${currentPopup.pop_id}"
                style="position: fixed; top: 0; left: 0; width: 100%; height: 100%;
                       background: rgba(0, 0, 0, 0.85); display: flex; justify-content: center;
                       align-items: center; z-index: 9999;">
                
                <div id="popup-box" style="position: relative; max-width: 400px; width: 90%;">
                    <button class="close-popup-btn" 
                        style="position: absolute; top: 10px; right: 10px; background: none;
                               border: none; font-size: 30px; color: white; cursor: pointer; line-height: 1;">
                        &times;
                    </button>

                    <img src="${currentPopup.image}" 
                         alt="Promotion Banner" 
                         style="width: 100%; height: auto; display: block; border: none; border-radius: 12px;"/>
                </div>
            </div>
        `;

        document.body.insertAdjacentHTML('beforeend', popupHtml);
        const overlay = document.getElementById(`popup-overlay-${currentPopup.pop_id}`);
        const closeButtons = overlay.querySelectorAll('.close-popup-btn');

        const closePopupAndProceed = () => {
            overlay.remove();
            showNextPopup(popups);
        };

        closeButtons.forEach(btn => btn.addEventListener('click', closePopupAndProceed));
    }

    // --- 3. Fetch & Display ---
    async function fetchAndDisplayPopups() {
        if (!shouldShowPopups()) {
            console.log("Popups are disabled by user preference.");
            return;
        }

        try {
            const response = await fetch(API_ENDPOINT);
            const result = await response.json();

            if (result.status && result.count > 0) {
                const pageName = document.body.dataset.pageName || 'home'; // optional dynamic page
                const pagePopups = result.data.filter(item => item.page_name.toLowerCase() === pageName);

                if (pagePopups.length > 0) {
                    console.log(`Found ${pagePopups.length} popups for page: ${pageName}`);
                    showNextPopup(pagePopups.slice());
                }
            }
        } catch (error) {
            console.error('Failed to fetch popup data:', error);
        }
    }

    // --- 4. Initialize ---
    document.addEventListener('DOMContentLoaded', fetchAndDisplayPopups);
</script>
<script>
const sliderContainer = document.getElementById("slider");
const dotsContainer = document.getElementById("dots");
const prevBtn = document.getElementById("prevBtn");
const nextBtn = document.getElementById("nextBtn");

let slides = [];
let dots = [];
let currentIndex = 0;
let interval;

// Fetch slider data from API
fetch("/api/webapi/slider")
  .then(res => res.json())
  .then(data => {
    if (data.status && data.count > 0) {
      data.data.forEach((slide, i) => {
        // Create slide image
        const img = document.createElement("img");
        img.src = slide.image_url;
        img.alt = slide.title;
        img.className = "absolute inset-0 w-full h-full object-fill transition-opacity duration-700";
        img.classList.add(i === 0 ? "opacity-100" : "opacity-0");

        // Make image clickable if page_url exists
        if (slide.page_url) {
          img.style.cursor = "pointer";
          img.addEventListener("click", () => {
            window.location.href = slide.page_url;
          });
        }

        sliderContainer.appendChild(img);
        slides.push(img);

        // Create dot
        const dot = document.createElement("span");
        dot.className = "w-8 h-1 rounded-full bg-[#FA605F] bg-opacity-50 cursor-pointer transition";
        if (i === 0) dot.classList.add("bg-opacity-100");
        dot.addEventListener("click", () => showSlide(i));
        dotsContainer.appendChild(dot);
        dots.push(dot);
      });

      startAutoSlide();
    }
  })
  .catch(err => console.error("Failed to load slider:", err));

function showSlide(index) {
  slides[currentIndex].classList.replace("opacity-100", "opacity-0");
  dots[currentIndex].classList.remove("bg-opacity-100");

  currentIndex = (index + slides.length) % slides.length;

  slides[currentIndex].classList.replace("opacity-0", "opacity-100");
  dots[currentIndex].classList.add("bg-opacity-100");
}

prevBtn.addEventListener("click", () => {
  showSlide(currentIndex - 1);
  resetAutoSlide();
});

nextBtn.addEventListener("click", () => {
  showSlide(currentIndex + 1);
  resetAutoSlide();
});

function startAutoSlide() {
  interval = setInterval(() => showSlide(currentIndex + 1), 4000);
}

function resetAutoSlide() {
  clearInterval(interval);
  startAutoSlide();
}
</script>


