<script>
    const initFunc = () => {
      console.log("Splash in sessionStorage:", sessionStorage.getItem("splash"));

      // Check if the splash screen has already been shown in this session
      if (sessionStorage.getItem("splash") === "true") {
        $(".splash_root").fadeOut(600); // Immediately hide the splash screen if already shown
        $("#alert_container").show(); // Show the alert content immediately
        return;
      }

      // Ensure the splash screen is visible initially
      $(".splash_root").css("display", "flex");

      // After 5 seconds, fade out the splash screen
      setTimeout(function () {
        $(".splash_root").fadeOut(700, function() {
          // Show the alert content after the splash screen has fully faded out
          $("#alert_container").fadeIn(300);
        });
        sessionStorage.setItem("splash", "true");
      }, 5000); // Set the splash screen display time to 5 seconds
    };

    // Run the function on page load
    $(document).ready(function() {
      initFunc();
    });
  </script>

