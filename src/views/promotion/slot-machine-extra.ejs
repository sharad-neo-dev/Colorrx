.rebate-example-table[data-v-cacbc52e] {
    border-collapse: collapse;
    margin: 0 auto;
    width: calc(100% - 5px);
}
.rebate-example-table td[data-v-cacbc52e] {
    border: 1px solid #fff;
    font-size: 16px;
    padding: 4px 6px;
}
  @media (max-width:425px){
    .sys-rebate-container {
      padding: 7px;
  }
  
  }

  
  
  
  
  
  </style>
</head>
